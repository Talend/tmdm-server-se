<?xml version="1.0" encoding="UTF-8"?>
<project default="wsdl_all" name="WSDLCompile-GenerateWSElements">
	
	<property name="package.name" value="talend.core.transformer.plugin.v2.tiscall.webservices"/>
	<property name="wsdl.file" value="src/wsdl/tis.wsdl"/>
	
	<property name="jboss.home" value="c:/opt/jboss"/>
	<property name="source.dir" value="./src"/>
	<property name="bin.dir" value="./bin"/>
	
	<target name="wsdl_all">
		<exec executable="${jboss.home}/bin/wsconsume.bat">
			<!--<arg line="-import"/>-->
			<arg line="-k"/>
			<arg line="-v"/>
			<arg line="-p ${package.name}"/>
			<arg line="-w http://localhost:8083/WSxml_0.1/services/WSxml?WSDL"/>
			<arg line="-o ${bin.dir} -s ${source.dir}"/>
			<arg line="${wsdl.file}"/>									
		</exec>	
	</target>

</project>