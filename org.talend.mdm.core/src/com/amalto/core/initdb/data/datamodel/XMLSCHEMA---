 &lt;xs:element id="annotation" name="annotation"&gt;
   &lt;xs:annotation&gt;
     &lt;xs:documentation source="http://www.w3.org/TR/xmlschema-1/#element-annotation"/&gt;
   &lt;/xs:annotation&gt;
