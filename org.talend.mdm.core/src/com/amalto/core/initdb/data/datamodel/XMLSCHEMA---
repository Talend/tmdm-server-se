 &lt;/xs:group&gt;
 
 &lt;xs:complexType name="attribute"&gt;
  &lt;xs:complexContent&gt;
   &lt;xs:extension base="xs:annotated"&gt;
    &lt;xs:sequence&gt;
     &lt;xs:element minOccurs="0" name="simpleType" type="xs:localSimpleType"/&gt;
    &lt;/xs:sequence&gt;
    &lt;xs:attributeGroup ref="xs:defRef"/&gt;
    &lt;xs:attribute name="type" type="xs:QName"/&gt;
    &lt;xs:attribute default="optional" name="use" use="optional"&gt;
     &lt;xs:simpleType&gt;
      &lt;xs:restriction base="xs:NMTOKEN"&gt;
       &lt;xs:enumeration value="prohibited"/&gt;
       &lt;xs:enumeration value="optional"/&gt;
       &lt;xs:enumeration value="required"/&gt;
      &lt;/xs:restriction&gt;
     &lt;/xs:simpleType&gt;
    &lt;/xs:attribute&gt;
    &lt;xs:attribute name="default" type="xs:string"/&gt;
    &lt;xs:attribute name="fixed" type="xs:string"/&gt;
    &lt;xs:attribute name="form" type="xs:formChoice"/&gt;
   &lt;/xs:extension&gt;
  &lt;/xs:complexContent&gt;
 &lt;/xs:complexType&gt;
 
 &lt;xs:complexType name="topLevelAttribute"&gt;
  &lt;xs:complexContent&gt;
   &lt;xs:restriction base="xs:attribute"&gt;
    &lt;xs:sequence&gt;
     &lt;xs:element minOccurs="0" ref="xs:annotation"/&gt;
     &lt;xs:element minOccurs="0" name="simpleType" type="xs:localSimpleType"/&gt;
    &lt;/xs:sequence&gt;
    &lt;xs:attribute name="ref" use="prohibited"/&gt;
    &lt;xs:attribute name="form" use="prohibited"/&gt;
    &lt;xs:attribute name="use" use="prohibited"/&gt;
    &lt;xs:attribute name="name" type="xs:NCName" use="required"/&gt;
    &lt;xs:anyAttribute namespace="##other" processContents="lax"/&gt;
   &lt;/xs:restriction&gt;
  &lt;/xs:complexContent&gt;
 &lt;/xs:complexType&gt;

 &lt;xs:group name="attrDecls"&gt;
  &lt;xs:sequence&gt;
   &lt;xs:choice maxOccurs="unbounded" minOccurs="0"&gt;
    &lt;xs:element name="attribute" type="xs:attribute"/&gt;
    &lt;xs:element name="attributeGroup" type="xs:attributeGroupRef"/&gt;
   &lt;/xs:choice&gt;
   &lt;xs:element minOccurs="0" ref="xs:anyAttribute"/&gt;
  &lt;/xs:sequence&gt;
 &lt;/xs:group&gt;

 &lt;xs:element id="anyAttribute" name="anyAttribute" type="xs:wildcard"&gt;
  &lt;xs:annotation&gt;
   &lt;xs:documentation source="http://www.w3.org/TR/xmlschema-1/#element-anyAttribute"/&gt;
  &lt;/xs:annotation&gt;
 &lt;/xs:element&gt;

 &lt;xs:group name="complexTypeModel"&gt;
  &lt;xs:choice&gt;
      &lt;xs:element ref="xs:simpleContent"/&gt;
      &lt;xs:element ref="xs:complexContent"/&gt;
      &lt;xs:sequence&gt;
       &lt;xs:annotation&gt;
        &lt;xs:documentation&gt;
   This branch is short for
   &amp;lt;complexContent&amp;gt;
   &amp;lt;restriction base="xs:anyType"&amp;gt;
   ...
   &amp;lt;/restriction&amp;gt;
   &amp;lt;/complexContent&amp;gt;&lt;/xs:documentation&gt;
       &lt;/xs:annotation&gt;
       &lt;xs:group minOccurs="0" ref="xs:typeDefParticle"/&gt;
