    <!-- INHERITANCE TYPE MODEL -->
    <xsd:element name="A" type="A">
        <xsd:unique name="A">
            <xsd:selector xpath="."/>
            <xsd:field xpath="id"/>
        </xsd:unique>
    </xsd:element>
    <xsd:element name="B" type="B">
        <xsd:unique name="B">
            <xsd:selector xpath="."/>
            <xsd:field xpath="id"/>
        </xsd:unique>
    </xsd:element>
    <xsd:element name="C" type="C">
        <xsd:unique name="C">
            <xsd:selector xpath="."/>
            <xsd:field xpath="id"/>
        </xsd:unique>
    </xsd:element>
    <xsd:element name="D" type="D">
        <xsd:unique name="D">
            <xsd:selector xpath="."/>
            <xsd:field xpath="id"/>
        </xsd:unique>
    </xsd:element>
    <xsd:complexType name="A">
        <xsd:sequence>
            <xsd:element maxOccurs="1" minOccurs="1" name="id" type="xsd:string"/>
            <xsd:element maxOccurs="1" minOccurs="1" name="textA" type="xsd:string"/>
            <xsd:element maxOccurs="1" minOccurs="1" name="refB" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo source="X_ForeignKey">B/id</xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="1" name="nestedB" type="Nested"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="B">
        <xsd:sequence>
            <xsd:element maxOccurs="1" minOccurs="1" name="id" type="xsd:string"/>
            <xsd:element maxOccurs="1" minOccurs="1" name="textB" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="C">
        <xsd:complexContent>
            <xsd:extension base="A">
                <xsd:sequence>
                    <xsd:element maxOccurs="1" minOccurs="1" name="textC" type="xsd:string"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="D">
        <xsd:complexContent>
            <xsd:extension base="B">
                <xsd:sequence>
                    <xsd:element maxOccurs="1" minOccurs="1" name="textD" type="xsd:string"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="Nested">
        <xsd:sequence>
            <xsd:element maxOccurs="1" minOccurs="1" name="text" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="SubNested">
        <xsd:complexContent>
            <xsd:extension base="Nested">
                <xsd:sequence>
                    <xsd:element maxOccurs="1" minOccurs="1" name="subText" type="xsd:string"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
</xsd:schema>
