<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">

    <!-- PERSON DATA MODEL -->

    <xsd:element name="Person">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="id" type="xsd:integer"/>
                <xsd:element name="firstname" type="xsd:string"/>
                <xsd:element name="middlename" type="xsd:string"/>
                <xsd:element name="lastname" type="xsd:string"/>
                <xsd:element name="age" type="xsd:positiveInteger"/>
                <xsd:element name="score" type="xsd:decimal"/>
                <xsd:element minOccurs="0" name="Available" type="xsd:boolean"/>
                <xsd:element maxOccurs="unbounded" minOccurs="1" name="addresses">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element maxOccurs="unbounded" name="address" type="xsd:string">
                                <xsd:annotation>
                                    <xsd:appinfo source="X_ForeignKey">Address</xsd:appinfo>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="1" name="Status" type="PersonStatus">
                    <xsd:annotation>
                        <xsd:appinfo source="X_Create">System_Admin</xsd:appinfo>
                        <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                        <xsd:appinfo source="X_Hide">System_Users</xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
        <xsd:unique name="Person">
            <xsd:selector xpath="."/>
            <xsd:field xpath="id"/>
        </xsd:unique>
    </xsd:element>

    <xsd:simpleType name="PersonStatus">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Customer"/>
            <xsd:enumeration value="Friend"/>
            <xsd:enumeration value="Employee"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:element name="Address">
        <xsd:annotation>
            <xsd:appinfo source="X_Label_FR">Adresse</xsd:appinfo>
            <xsd:appinfo source="X_Label_EN">Address</xsd:appinfo>
            <xsd:appinfo source="X_Create">System_Admin</xsd:appinfo>
            <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all maxOccurs="1" minOccurs="1">
                <xsd:element maxOccurs="1" minOccurs="1" name="Id" type="xsd:string">
                    <xsd:annotation>
                        <xsd:appinfo source="X_Label_FR">Id (s?quence)</xsd:appinfo>
                        <xsd:appinfo source="X_Label_EN">Id (sequence)</xsd:appinfo>
                        <xsd:appinfo source="X_Create">System_Admin</xsd:appinfo>
                        <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="1" name="Street" type="xsd:string">
                    <xsd:annotation>
                        <xsd:appinfo source="X_Label_FR">Rue</xsd:appinfo>
                        <xsd:appinfo source="X_Label_EN">Street</xsd:appinfo>
                        <xsd:appinfo source="X_Create">System_Admin</xsd:appinfo>
                        <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="1" name="ZipCode" type="xsd:string">
                    <xsd:annotation>
                        <xsd:appinfo source="X_Label_FR">Code postal</xsd:appinfo>
                        <xsd:appinfo source="X_Label_EN">Zip code</xsd:appinfo>
                        <xsd:appinfo source="X_Create">System_Admin</xsd:appinfo>
                        <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="1" name="enterprise" type="xsd:boolean">
                    <xsd:annotation>
                        <xsd:appinfo source="X_Label_FR">Code postal</xsd:appinfo>
                        <xsd:appinfo source="X_Label_EN">Zip code</xsd:appinfo>
                        <xsd:appinfo source="X_Create">System_Admin</xsd:appinfo>
                        <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="1" name="City" type="xsd:string">
                    <xsd:annotation>
                        <xsd:appinfo source="X_Label_FR">Ville</xsd:appinfo>
                        <xsd:appinfo source="X_Label_EN">City</xsd:appinfo>
                        <xsd:appinfo source="X_Create">System_Admin</xsd:appinfo>
                        <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="OptionalCity" type="xsd:string">
                    <xsd:annotation>
                        <xsd:appinfo source="X_Label_FR">Ville optionelle</xsd:appinfo>
                        <xsd:appinfo source="X_Label_EN">Optional city</xsd:appinfo>
                        <xsd:appinfo source="X_Create">System_Admin</xsd:appinfo>
                        <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="1" name="country" type="xsd:string">
                    <xsd:annotation>
                        <xsd:appinfo source="X_ForeignKey">Country/id</xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
            </xsd:all>
        </xsd:complexType>
        <xsd:unique name="Address">
            <xsd:selector xpath="."/>
            <xsd:field xpath="Id"/>
            <xsd:field xpath="enterprise"/>
        </xsd:unique>
    </xsd:element>

    <xsd:element name="Country">
        <xsd:complexType>
            <xsd:all>
                <xsd:element name="id" type="xsd:integer"/>
                <xsd:element name="creationDate" type="xsd:date"/>
                <xsd:element name="creationTime" type="xsd:dateTime"/>
                <xsd:element name="name" type="xsd:string"/>
            </xsd:all>
        </xsd:complexType>
        <xsd:unique name="Country">
            <xsd:selector xpath="."/>
            <xsd:field xpath="id"/>
        </xsd:unique>
    </xsd:element>

