            <xsd:appinfo source="X_Deny_PhysicalDelete">RAP_SRC</xsd:appinfo>
            <xsd:appinfo source="X_Deny_Create">RAP_ReadOnly</xsd:appinfo>
            <xsd:appinfo source="X_Deny_Create">RAP_DCO</xsd:appinfo>
            <xsd:appinfo source="X_Deny_LogicalDelete">RAP_ReadOnly</xsd:appinfo>
            <xsd:appinfo source="X_Deny_LogicalDelete">RAP_DCO</xsd:appinfo>
            <xsd:appinfo source="X_Deny_LogicalDelete">RAP_SRC</xsd:appinfo>
            <xsd:appinfo source="X_Schematron">&lt;pattern name="SiteInjectionRpd" &gt; &lt;rule context="/"&gt;&lt;assert test="translate(dateDebut,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;) = &amp;quot;&amp;quot; or translate(dateFin,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;) = &amp;quot;&amp;quot; or number( translate( dateDebut, &amp;quot;-&amp;quot;, &amp;quot;&amp;quot;) ) &amp;lt;number( translate( dateFin, &amp;quot;-&amp;quot;, &amp;quot;&amp;quot;) )"&gt;&lt;![CDATA[[FR:La date de mise en service doit être inférieure à la date de fin.]]]&gt;&lt;/assert&gt;&lt;/rule&gt; &lt;rule context="REs/RE"&gt;&lt;assert test="translate(dateDebutApplication,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;) = &amp;quot;&amp;quot; or translate(dateFinApplication,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;) = &amp;quot;&amp;quot; or number( translate( dateDebutApplication, &amp;quot;-&amp;quot;, &amp;quot;&amp;quot;) ) &amp;lt;number( translate( dateFinApplication, &amp;quot;-&amp;quot;, &amp;quot;&amp;quot;) )"&gt;&lt;![CDATA[[FR:La date de début d'application rattachement RE doit être inférieure à la date de fin d'application rattachement RE.]]]&gt;&lt;/assert&gt;&lt;/rule&gt; &lt;rule context="fournisseurs/fournisseur"&gt;&lt;assert test="translate(dateDebut,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;) = &amp;quot;&amp;quot; or translate(dateFin,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;) = &amp;quot;&amp;quot; or number( translate( dateDebut, &amp;quot;-&amp;quot;, &amp;quot;&amp;quot;) ) &amp;lt;number( translate( dateFin, &amp;quot;-&amp;quot;, &amp;quot;&amp;quot;) )"&gt;&lt;![CDATA[[FR:La date de début d'application rattachement Fournisseur doit être inférieure à la date de fin d'application rattachement Fournisseur.]]]&gt;&lt;/assert&gt;&lt;/rule&gt; &lt;/pattern&gt;</xsd:appinfo>
        </xsd:annotation>
        <xsd:unique name="SiteInjectionRpd">
            <xsd:selector xpath="." />
            <xsd:field xpath="idSite" />
        </xsd:unique>
    </xsd:element>
