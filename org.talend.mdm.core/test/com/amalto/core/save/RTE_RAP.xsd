            <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
            <xsd:appinfo source="X_Write">User</xsd:appinfo>
            <xsd:appinfo source="X_Write">RAP_SRC</xsd:appinfo>
            <xsd:appinfo source="X_PrimaryKeyInfo">Edp/codeEdp</xsd:appinfo>
            <xsd:appinfo source="X_PrimaryKeyInfo">Edp/nomEdp</xsd:appinfo>
            <xsd:appinfo source="X_Deny_Create">RAP_ReadOnly</xsd:appinfo>
            <xsd:appinfo source="X_Deny_Create">RAP_DCO</xsd:appinfo>
            <xsd:appinfo source="X_Deny_PhysicalDelete">RAP_ReadOnly</xsd:appinfo>
            <xsd:appinfo source="X_Deny_PhysicalDelete">RAP_DCO</xsd:appinfo>
            <xsd:appinfo source="X_Deny_PhysicalDelete">RAP_SRC</xsd:appinfo>
            <xsd:appinfo source="X_Deny_LogicalDelete">RAP_DCO</xsd:appinfo>
            <xsd:appinfo source="X_Deny_LogicalDelete">RAP_ReadOnly</xsd:appinfo>
            <xsd:appinfo source="X_Deny_LogicalDelete">RAP_SRC</xsd:appinfo>
            <xsd:appinfo source="X_Schematron">&lt;pattern name="SsyDateValidation" &gt; &lt;rule context="dateDebutSsyEdp"&gt;&lt;assert test="translate(.,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;) = &amp;quot;&amp;quot; or number(translate(.,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;)) &gt;= number(translate(../datedebutEdp,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;)) and (translate(../datefinEdp,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;) = &amp;quot;&amp;quot; or number(translate(.,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;)) &amp;lt; number(translate(../datefinEdp,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;)))"&gt;&lt;![CDATA[[FR:La date début Ssy doit être comprise entre la date début Edp (inclusif) et la date fin Edp (exclusif).]]]&gt;&lt;/assert&gt;&lt;/rule&gt; &lt;rule context="dateFinSsyEdp"&gt;&lt;assert test="translate(.,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;) = &amp;quot;&amp;quot; or number(translate(.,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;)) &gt;= number(translate(../datedebutEdp,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;)) and (translate(../datefinEdp,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;) = &amp;quot;&amp;quot; or number(translate(.,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;)) &amp;lt;= number(translate(../datefinEdp,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;)))"&gt;&lt;![CDATA[[FR:La date fin Ssy doit être comprise entre la date début Edp (inclusif) et la date fin Edp (inclusif).]]]&gt;&lt;/assert&gt;&lt;/rule&gt; &lt;/pattern&gt;</xsd:appinfo>
            <xsd:appinfo source="X_Schematron">&lt;pattern name="DateFinValidation" &gt; &lt;rule context="/"&gt;&lt;assert test="translate(datedebutEdp,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;) = &amp;quot;&amp;quot; or translate(datefinEdp,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;) = &amp;quot;&amp;quot; or number( translate( datedebutEdp, &amp;quot;-&amp;quot;, &amp;quot;&amp;quot;) ) &amp;lt;number( translate( datefinEdp, &amp;quot;-&amp;quot;, &amp;quot;&amp;quot;) )"&gt;&lt;![CDATA[[FR: La date de début Edp doit être inférieure à la date de fin Edp.]]]&gt;&lt;/assert&gt;&lt;/rule&gt; &lt;rule context="/"&gt;&lt;assert test="translate(dateDebutSsyEdp,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;) = &amp;quot;&amp;quot; or translate(dateFinSsyEdp,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;) = &amp;quot;&amp;quot; or number( translate( dateDebutSsyEdp, &amp;quot;-&amp;quot;, &amp;quot;&amp;quot;) ) &amp;lt;number( translate( dateFinSsyEdp, &amp;quot;-&amp;quot;, &amp;quot;&amp;quot;) )"&gt;&lt;![CDATA[[FR: La date de début Ssy doit être inférieure à la date de fin Ssyy.]]]&gt;&lt;/assert&gt;&lt;/rule&gt; &lt;/pattern&gt;</xsd:appinfo>
            <xsd:appinfo source="X_Schematron">&lt;pattern name="codeEdpValidation" &gt; &lt;rule context="/"&gt;&lt;assert test="( controleCodeEdp=&amp;quot;Oui&amp;quot; and string-length(codeEdp) &gt;=6 and string-length(codeEdp)&amp;lt;=8 ) or controleCodeEdp=&amp;quot;Non&amp;quot; or fn:empty(controleCodeEdp)"&gt;&lt;![CDATA[[FR:Erreur sur le code de l'Edp]]]&gt;&lt;/assert&gt;&lt;/rule&gt; &lt;/pattern&gt;</xsd:appinfo>
        </xsd:annotation>
        <xsd:unique name="Edp">
            <xsd:selector xpath="." />
            <xsd:field xpath="idEdp" />
        </xsd:unique>
    </xsd:element>
