            <xsd:appinfo source="X_PrimaryKeyInfo">Edprev/codeEdprev</xsd:appinfo>
            <xsd:appinfo source="X_PrimaryKeyInfo">Edprev/nomEdprev</xsd:appinfo>
            <xsd:appinfo source="X_Deny_PhysicalDelete">RAP_ReadOnly</xsd:appinfo>
            <xsd:appinfo source="X_Deny_PhysicalDelete">RAP_DCO</xsd:appinfo>
            <xsd:appinfo source="X_Deny_PhysicalDelete">RAP_SRC</xsd:appinfo>
            <xsd:appinfo source="X_Deny_Create">RAP_ReadOnly</xsd:appinfo>
            <xsd:appinfo source="X_Deny_Create">RAP_DCO</xsd:appinfo>
            <xsd:appinfo source="X_Deny_LogicalDelete">RAP_ReadOnly</xsd:appinfo>
            <xsd:appinfo source="X_Deny_LogicalDelete">RAP_DCO</xsd:appinfo>
            <xsd:appinfo source="X_Deny_LogicalDelete">RAP_SRC</xsd:appinfo>
            <xsd:appinfo source="X_Schematron">&lt;pattern name="DateFinValidation" &gt; &lt;rule context="/"&gt;&lt;assert test="translate(datedebutEdprev,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;) = &amp;quot;&amp;quot; or translate(datefinEdprev,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;) = &amp;quot;&amp;quot; or number( translate( datedebutEdprev, &amp;quot;-&amp;quot;, &amp;quot;&amp;quot;) ) &amp;lt; number( translate( datefinEdprev, &amp;quot;-&amp;quot;, &amp;quot;&amp;quot;))"&gt;&lt;![CDATA[[FR:La date de début de l'Edprev doit être inférieure à la date de fin de l'Edprev.]]]&gt;&lt;/assert&gt;&lt;/rule&gt; &lt;rule context="/"&gt;&lt;assert test="translate(dateDebutSsyEdprev,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;) = &amp;quot;&amp;quot; or translate(dateFinSsyEdprev,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;) = &amp;quot;&amp;quot; or number( translate( dateDebutSsyEdprev, &amp;quot;-&amp;quot;, &amp;quot;&amp;quot;) ) &amp;lt;number( translate( dateFinSsyEdprev, &amp;quot;-&amp;quot;, &amp;quot;&amp;quot;) )"&gt;&lt;![CDATA[[FR: La date de début Ssy doit être inférieure à la date de fin Ssy.]]]&gt;&lt;/assert&gt;&lt;/rule&gt; &lt;/pattern&gt;</xsd:appinfo>
            <xsd:appinfo source="X_Schematron">&lt;pattern name="SsyDateValidation" &gt; &lt;rule context="dateDebutSsyEdprev"&gt;&lt;assert test="translate(.,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;) = &amp;quot;&amp;quot; or number(translate(.,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;)) &gt;= number(translate(../datedebutEdprev,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;)) and (translate(../datefinEdprev,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;) = &amp;quot;&amp;quot; or number(translate(.,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;)) &amp;lt; number(translate(../datefinEdprev,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;)))"&gt;&lt;![CDATA[[FR:La date début Ssy doit être comprise entre la date début Edprev (inclusif) et la date fin Edprev (exclusif).]]]&gt;&lt;/assert&gt;&lt;/rule&gt; &lt;rule context="dateFinSsyEdprev"&gt;&lt;assert test="translate(.,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;) = &amp;quot;&amp;quot; or number(translate(.,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;)) &gt;= number(translate(../datedebutEdprev,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;)) and (translate(../datefinEdprev,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;) = &amp;quot;&amp;quot; or number(translate(.,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;)) &amp;lt;= number(translate(../datefinEdprev,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;)))"&gt;&lt;![CDATA[[FR:La date fin Ssy doit être comprise entre la date début Edprev (inclusif) et la date fin Edprev (inclusif).]]]&gt;&lt;/assert&gt;&lt;/rule&gt; &lt;/pattern&gt;</xsd:appinfo>
            <xsd:appinfo source="X_Schematron">&lt;pattern name="codeEdprevValidation" &gt; &lt;rule context="/"&gt;&lt;assert test="( controleCodeEdprev=&amp;quot;Oui&amp;quot; and string-length(codeEdprev) &gt;=6 and string-length(codeEdprev)&amp;lt;=8 ) or controleCodeEdprev=&amp;quot;Non&amp;quot; or fn:empty(controleCodeEdprev)"&gt;&lt;![CDATA[[FR:Erreur sur le code de l'Edprev]]]&gt;&lt;/assert&gt;&lt;/rule&gt; &lt;/pattern&gt;</xsd:appinfo>
        </xsd:annotation>
        <xsd:unique name="Edprev">
            <xsd:selector xpath="." />
            <xsd:field xpath="idEdprev" />
        </xsd:unique>
    </xsd:element>
