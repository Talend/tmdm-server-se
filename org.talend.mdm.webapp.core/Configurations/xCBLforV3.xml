                <Name>Order</Name>
                <Description>
                    <EN>Purchase Order</EN>
                    <FR>Commande</FR>
                </Description>
                <ConceptName>Order</ConceptName>
                <ListOfKeys>
                    <Key>
                        <Name>Buyer Order Number</Name>
                        <Description>
                            <EN>Purchase Order Number</EN>
                            <FR>Numéro de la Commande</FR>
                        </Description>
                        <XPath>Order/OrderHeader/OrderNumber/BuyerOrderNumber</XPath>
                    </Key>
                    <Key>
                        <Name>Buyer Party ID</Name>
                        <Description>
                            <EN>Buyer Party Identification</EN>
                            <FR>Identifiant de l'acheteur</FR>
                        </Description>
                        <XPath>Order/OrderHeader/OrderParty/BuyerParty/Party/PartyID/Identifier/Ident</XPath>
                    </Key>
                    <Key>
                        <Name>Seller Party ID</Name>
                        <Description>
                            <EN>Seller Party Identification</EN>
                            <FR>Identifiant du fournisseur</FR>
                        </Description>
                        <XPath>Order/OrderHeader/OrderParty/SellerParty/Party/PartyID/Identifier/Ident</XPath>
                    </Key>
