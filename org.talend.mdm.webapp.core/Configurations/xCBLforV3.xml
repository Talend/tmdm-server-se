                    <Viewable>
                        <Name>InsertionTime</Name>
                        <Description>
                            <EN>Received Date</EN>
                            <FR>Date de réception</FR>
                        </Description>
                        <XPath>Order/../../t</XPath>
                        <Type>Millis</Type>
                    </Viewable>
                    <Viewable>
                        <Name>Order Date</Name>
