<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dwr PUBLIC "-//GetAhead Limited//DTD Direct Web Remoting 2.0//EN" "http://getahead.org/dwr/dwr20.dtd">
<dwr>
	<allow>
		
		<create creator="new" javascript="GroupingHierarchyInterface">
			<param name="class" value="com.amalto.webapp.v3.hierarchical.dwr.HierarchicalDWR" />
		</create>
		
		<convert converter="bean" match="com.amalto.webapp.core.bean.ComboItemBean"/>
		<convert converter="bean" match="com.amalto.webapp.core.bean.ListRange"/>
		<convert converter="bean" match="com.amalto.webapp.v3.hierarchical.bean.FilterItem"/>
		<convert converter="bean" match="java.lang.Exception"/>
		<convert converter="bean" match="java.lang.StackTraceElement"/>
		<convert converter="bean" match="com.amalto.webapp.v3.hierarchical.bean.HierarchicalReport"/>
		<convert converter="bean" match="com.amalto.webapp.v3.hierarchical.bean.Filters"/>
		
		
	</allow>
</dwr>