<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
    <servlet>
        <servlet-name>recyclebinServlet</servlet-name>
        <servlet-class>org.talend.mdm.webapp.general.gwt.ProxyGWTServiceImpl</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>recyclebinServlet</servlet-name>
        <url-pattern>/recyclebin/RecycleBinService</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>RecycleBin.html</welcome-file>
    </welcome-file-list>
</web-app>
