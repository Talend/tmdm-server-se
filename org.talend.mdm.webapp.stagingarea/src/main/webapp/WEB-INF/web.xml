<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
        PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
    <servlet>
        <servlet-name>stagingareaServlet</servlet-name>
        <servlet-class>org.talend.mdm.webapp.general.gwt.ProxyGWTServiceImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StagingAreaBrowseService</servlet-name>
        <servlet-class>org.talend.mdm.webapp.general.gwt.ProxyGWTServiceImpl</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>stagingareaServlet</servlet-name>
        <url-pattern>/stagingarea/StagingAreaService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StagingAreaBrowseService</servlet-name>
        <url-pattern>/stagingareabrowse/StagingAreaBrowseService</url-pattern>
    </servlet-mapping>

    <!-- Default page to serve -->
    <welcome-file-list>
        <welcome-file>Stagingarea.html</welcome-file>
    </welcome-file-list>
</web-app>
